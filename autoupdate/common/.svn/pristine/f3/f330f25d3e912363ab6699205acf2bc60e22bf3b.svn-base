#ifndef INCLUDED_STREAM_TYPE_DEF
#define INCLUDED_STREAM_TYPE_DEF

#pragma once

#undef 	u32
#undef 	u16
#undef  u8



//8字节整数
#ifdef WIN32
typedef unsigned __int64 u64;
#else
typedef unsigned long long u64;
#endif

//4字节整数
typedef unsigned long u32;
//2字节整数
typedef unsigned short u16;
//1字节整数
typedef unsigned char  u8;



#endif // INCLUDED_STREAM_TYPE_DEF
